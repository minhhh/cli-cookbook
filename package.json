{
  "name": "cli-cookbook",
  "version": "0.0.0",
  "description": "A cookbook for command line tools.",
  "main": "index.js",
  "scripts": {
    "generate": "git clean -df && $(npm bin)/gitbook build . ./gh-pages",
    "serve": "$(npm bin)/gitbook serve",
    "deploy": "npm run generate && gh-pages -d gh-pages -b gh-pages && git clean -df && git push"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/minhhh/js-cookbook.git"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "author": "",
  "keywords": [
    "gitbook",
    "cli",
    "command",
    "cookbook",
    "js"
  ],
  "license": "MIT",
  "homepage": "https://github.com/minhhh/cli-cookbook",
  "dependencies": {
    "gh-pages-cli": "^0.2.0",
    "gitbook-cli": "^2.3.2",
    "yarn": "^1.17.3"
  },
  "devDependencies": {},
  "bugs": {
    "url": "https://github.com/minhhh/js-cookbook/issues"
  }
}
